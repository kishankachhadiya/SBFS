<p id="notice"><%= notice %></p>

<h1>Kind Of Boats</h1>

<table>
  <thead>
    <tr>
      <th>Kind</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kind_of_boats.each do |kind_of_boat| %>
      <tr>
        <td><%= kind_of_boat.kind %></td>
        <td><%= link_to 'Show', kind_of_boat %></td>
        <td><%= link_to 'Edit', edit_kind_of_boat_path(kind_of_boat) %></td>
        <td><%= link_to 'Destroy', kind_of_boat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kind Of Boat', new_kind_of_boat_path %>
